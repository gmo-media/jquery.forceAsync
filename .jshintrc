{
    // 強制オプション
    "bitwise":           true,      // ビット演算子の使用禁止
    "camelcase":        false,      // camelCase もしくは UPPER_CASE の強制
    "curly":             true,      // 波括弧の強制
    "eqeqeq":            true,      // 同値演算子の強制（等値演算子の使用禁止）
    "freeze":            true,      // 組み込みオブジェクトのprototype上書き禁止
    "immed":             true,      // 即時関数の括弧強制
    "indent":           false,      // タブインデントの強制
    "latedef":           true,      // 定義前変数の使用禁止
    "maxparams":            3,      // 関数の引数の最大数を指定
    "maxdepth":             3,      // 入れ子の深さの最大値を指定
    "maxstatements":       10,      // 関数内の文の最大数を指定
    "maxcomplexity":        5,      // 循環的複雑度の最大値を指定
    "maxlen":              80,      // 一行の最大の長さを指定
    "newcap":            true,      // コンストラクタ関数の先頭大文字強制
    "noarg":             true,      // arguments.callerおよびarguments.calleeの使用禁止
    "noempty":           true,      // 空ブロックの警告
    "nonbsp":            true,      // 改行防止スペース（non-breaking whitespace）の禁止
    "nonew":             true,      // 代入しないコンストラクタ関数の使用禁止
    "plusplus":         false,      // ++、--の使用禁止
    "quotmark":      "single",      // クオテーション統一
    "strict":           false,      // ECMAScript5 の strict mode 強制
    "trailing":          true,      // 行末の余分な空白エラー
    "undef":             true,      // 宣言していない変数の使用禁止
    "unused":            true,      // 使用していない変数について警告する

    // 緩和オプション
    "asi":               true,      // セミコロンの付け忘れについての警告停止
    "boss":              true,      // 比較演算子==の代入演算子=への書き間違えについての警告停止
    "debug":            false,      // debugger文についての警告停止
    "eqnull":           false,      // == nullについての警告停止
    "expr":              true,      // 式の中での代入もしくは関数呼出しについての警告停止
    "funcscope":        false,      // 外側からアクセス可能な構造内部での変数宣言についての警告停止
    "evil":             false,      // evalの使用についての警告停止
    "gcl":              false,      // Google Closure Compiler互換
    "globalstrict":     false,      // グローバルでのstrict mode使用についての警告停止
    "iterator":         false,      // __iterator__プロパティについての警告停止
    "lastsemic":         true,      // 一行の最後の文でのセミコロン忘れを除き、セミコロン漏れの警告停止
    "laxbreak":          true,      // 安全とは言えない改行についての警告停止
    "laxcomma":          true,      // 行頭カンマについての警告停止
    "loopfunc":         false,      // ループ内関数についての警告停止
    "maxerr":              50,      // エラー・警告の最大数
    "moz":              false,      // MozillaのJavaScript エクステンションを使用するときに設定
    "multistr":         false,      // 複数行文字列についての警告停止
    "notypeof":         false,      // typeof演算子の無効な値についての警告停止
    "proto":            false,      // __proto__`プロパティについての警告停止
    "scripturl":        false,      // スクリプト向けURL（こんなのjavascript:...）の使用についての警告停止
    "smarttabs":        false,      // タブとスペースの混在についての警告停止
    "shadow":           false,      // 変数隠しについての警告停止
    "sub":              false,      // ドット演算子で記述可能な箇所でのブラケット演算子[]の使用についての警告停止
    "supernew":         false,      // 「幽霊 (wierd)」コンストラクタについての警告停止
    "validthis":        false,      // strict mode時のコンストラクタでない関数でthisを使用するという厳密には違反になるコードについての警告停止

    // 環境に関するオプション
    "browser":           true,      // モダンブラウザ指定
    "couch":            false,      // CouchDB指定
    "devel":             true,      // consoleやalertなどのデバッグ用ログの使用を指定
    "dojo":             false,      // Dojo Toolkit指定
    "jquery":            true,      // jQueryライブラリ使用指定
    "mootools":         false,      // Mootoolsフレームワーク使用指定
    "node":             false,      // Node環境での実行指定
    "nonstandard":      false,      // 非標準指定
    "phantom":          false,      // PhantomJS環境での実行指定
    "prototypejs":      false,      // Prototypeフレームワーク使用指定
    "rhino":            false,      // Rhino環境での実行指定
    "worker":           false,      // Web Worker環境での実行指定
    "wsh":              false,      // Windows Script Hostでの実行指定
    "yui":              false,      // YUIフレームワーク使用指定

    // レガシーなオプション
    "nomen":            false,      // 末尾に_を付与した変数の不許可
    "onevar":           false,      // var文を一つにまとめることを強制
    "passfail":         false,      // 最初のエラーもしくは警告でJSHintを停止
    "white":            false       // Douglas Crockford氏のコーディングスタイルに反したコードのチェック
}
// see: http://www.jshint.com/docs/options/
// see: http://qiita.com/HieroglypH/items/3cb16ecff43b8e5f62e1
